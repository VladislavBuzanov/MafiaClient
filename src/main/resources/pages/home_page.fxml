<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.informatics.jojomafia.controllers.HomePageController">
    <children>
        <ImageView fx:id="imageView" fitHeight="720.0" fitWidth="1280.0">
            <image>
                <Image url="@../img/home_page.png" />
            </image>
        </ImageView>
        <Button fx:id="loginButton" layoutX="330.0" layoutY="360.0" mnemonicParsing="false" onAction="#sentToLoginPage" styleClass="btn-primary">
         <graphic>
            <ImageView fitHeight="38.0" fitWidth="150.0">
               <image>
                  <Image url="@../img/sign_in.png" />
               </image>
            </ImageView>
         </graphic></Button>
        <Button fx:id="registerButton" layoutX="805.0" layoutY="360.0" mnemonicParsing="false" onAction="#sendToRegisterPage" styleClass="btn btn-primary btn-lg">
         <graphic>
            <ImageView fitHeight="38.0" fitWidth="150.0">
               <image>
                  <Image url="@../img/sign_up.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <ImageView layoutY="606.0">
         <image>
            <Image url="@../img/ani.png" />
         </image>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <ImageView layoutX="912.0" layoutY="593.0">
         <image>
            <Image url="@../img/Mafia.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
